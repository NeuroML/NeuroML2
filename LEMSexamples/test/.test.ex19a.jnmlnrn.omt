# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: ../LEMS_NML2_Ex19a_GapJunctionInstances.xml 
engine: jNeuroML_NEURON
mep: .test.ex19.mep
experiments:
  cell0:
    observables:
      spike times:
        file: 
          path: ../results/ex19_v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: -69.5
        tolerance: 0.000319
  cell1:
    observables:
      spike times:
        file: 
          path: ../results/ex19_v.dat
          columns: [0,2]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: -69.5
        tolerance: 0.000344
  
