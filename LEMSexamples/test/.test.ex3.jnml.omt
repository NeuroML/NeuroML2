# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: ../LEMS_NML2_Ex3_Net.xml 
engine: jNeuroML
mep: .test.ex3.mep
experiments:
  syn1:
    observables:
      spike times:
        file: 
          path: ../results/ex3_v.dat
          columns: [0,1]
          scaling: [1000, 1]
        spike detection: 
          method: threshold
          threshold: -0.0515
        tolerance: 0.0031618887015178268
  syn2:
    observables:
      spike times:
        file: 
          path: ../results/ex3_v.dat
          columns: [0,2]
          scaling: [1000, 1]
        spike detection: 
          method: threshold
          threshold:  -0.0515
        tolerance: 0.003282507412113535
