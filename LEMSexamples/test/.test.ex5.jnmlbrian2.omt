# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: ../LEMS_NML2_Ex5_DetCell.xml 
engine: jNeuroML_Brian2
mep: .test.ex5.mep
experiments:
  v:
    observables:
      spike times:
        file: 
          path: ../results/ex5_v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0.001712
  m:
    observables:
      spike times:
        file: 
          path: ../results/ex5_vars.dat
          columns: [0,1]
          scaling: [1000, 1]
        spike detection: 
          method: threshold
          threshold: 0.9
        tolerance: 0.00166
