# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation
 
target: ../LEMS_NML2_Ex9_FN.xml 
engine: jNeuroML_NEURON
mep: .test.ex9.mep
experiments:
  V:
    observables:
      spike times:
        file: 
          path: ../results/ex9.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection: 
          method: threshold
          threshold: 0
        tolerance: 0
  W:
    observables:
      spike times:
        file: 
          path: ../results/ex9.dat
          columns: [0,2]
          scaling: [1000, 1]
        spike detection: 
          method: threshold
          threshold: 0.9
        tolerance: 0
