# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: ../LEMS_NML2_Ex24_FractionalConductance.xml
engine: jNeuroML
mep: .test.ex24.mep
experiments:
  v:
    observables:
      spike times:
        file:
          path: ../results/ex24_v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection:
          method: threshold
          threshold: 0
        tolerance: 0.0
