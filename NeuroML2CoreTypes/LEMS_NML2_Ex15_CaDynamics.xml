        <Display id="d3" title="Ex15: Ca Dynamics: NaF, KDr, CaHVA, KA current densities (uA_per_cm2)" timeScale="1ms">
            <Line id="l2" quantity="hhpop[0]/biophys/membraneProperties/Gran_NaF_98_all/iDensity" scale="1uA_per_cm2"  color="#000000" save="examples/testResults/ex15_nai.dat"/>
            <Line id="l3" quantity="hhpop[0]/biophys/membraneProperties/Gran_KDr_98_all/iDensity" scale="1uA_per_cm2"  color="#0000ff" save="examples/testResults/ex15_kdri.dat"/>
            <Line id="l5" quantity="hhpop[0]/biophys/membraneProperties/Gran_CaHVA_98_all/iDensity" scale="1uA_per_cm2"  color="#00ff00" save="examples/testResults/ex15_cai.dat"/>
            <Line id="l4" quantity="hhpop[0]/biophys/membraneProperties/Gran_KA_98_all/iDensity" scale="1uA_per_cm2"  color="#ff0000" save="examples/testResults/ex15_kai.dat"/>
        </Display>

        <Display id="d3" title="Ex15: Ca Dynamics: Ca concentration (mM)" timeScale="1ms">
            <Line id="li12" quantity="hhpop[0]/caConc" scale="1 mM"  color="#ffffff" save="examples/testResults/ex15_c2.dat"/>
        </Display>

        <Display id="d4" title="Ex15: Ca Dynamics: current totals" timeScale="1ms">
            <Line id="l0" quantity="hhpop[0]/pulseGen1/i" scale="1nA"  color="#dddddd" save="examples/testResults/ex15_naigc.dat"/>
            <Line id="l2" quantity="hhpop[0]/iCa" scale="1nA"  color="#00aa00" save="examples/testResults/ex15_naic2.dat"/>
        </Display>-->


    </Simulation>


</Lems>