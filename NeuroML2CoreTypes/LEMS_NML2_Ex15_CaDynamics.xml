        </Dynamics>
    </ComponentType>

    <ComponentType name = "Gran_KCa_98_m_beta_rate" extends = "baseVoltageConcDepRate">
        <Constant name = "TIME_SCALE" dimension = "time" value = "1 s"/>
        <Constant name = "VOLT_SCALE" dimension = "voltage" value = "1 V"/>
        <Constant name = "CONC_SCALE" dimension = "concentration" value = "1 mol_per_m3"/>
        <Constant name = "offset" dimension = "voltage" value = "0.010V"/>
        <Dynamics>
            <DerivedVariable name = "V" dimension = "none" value = "(v - offset) / VOLT_SCALE"/>
            <DerivedVariable name = "CACONC" dimension = "none" value = "caConc / CONC_SCALE"/>
            <DerivedVariable name = "r" exposure = "r" dimension = "per_time" value = "(1500/(1 + (CACONC / (1.5e-4 * (exp (-77*V)))))) / TIME_SCALE"/>
        </Dynamics>
    </ComponentType>


    <concentrationModel id="Gran_CaPool" type="decayingPoolConcentrationModel"
                         restingConc="7.55E-11 mol_per_cm3"
                         decayConstant="10.0 ms"
                         shellThickness="8.4E-6 cm"/>
-->

    <cell id="Granule_98">

        <notes>An implementation using ChannelML of the Granule cell mode from Maex, R and De Schutter, E. Synchronization of Golgi and Granule Cell Firing in a Detailed Network Model of the Cerebellar Granule Cell Layer, 1998</notes>

        <morphology id="morphology_Granule_98">

            <segment id="0" name="Soma">
                <proximal x="0.0" y="0.0" z="0.0" diameter="10.0"/>
                <distal x="0.0" y="0.0" z="0.0" diameter="10.0"/>
            </segment>

            <segmentGroup id="Soma">
                <member segment="0"/>
            </segmentGroup>

            <segmentGroup id="all">
                <include segmentGroup="Soma"/>
            </segmentGroup>

            <segmentGroup id="soma_group">
                <include segmentGroup="Soma"/>
            </segmentGroup>

            
        </morphology>

            <!--Adding the biophysical parameters-->

        <biophysicalProperties id="biophys">

            <membraneProperties>

                <channelDensity condDensity="0.0330033 mS_per_cm2" id="GranPassiveCond_all" ionChannel="GranPassiveCond" erev="-65.0 mV" ion="non_specific"/>

                <channelDensity condDensity="8.89691 mS_per_cm2" id="Gran_KDr_98_all" ionChannel="Gran_KDr_98" erev="-90.0 mV" ion="k"/>
                <channelDensity condDensity="0.03090506 mS_per_cm2" id="Gran_H_98_all" ionChannel="Gran_H_98" erev="-42.0 mV" ion="h"/>
                <channelDensity condDensity="0.9084216 mS_per_cm2" id="Gran_CaHVA_98_all" ionChannel="Gran_CaHVA_98" erev="80.0 mV" ion="ca"/>
                <channelDensity condDensity="1.14567 mS_per_cm2" id="Gran_KA_98_all" ionChannel="Gran_KA_98" erev="-90.0 mV" ion="k"/>
                <channelDensity condDensity="0 mS_per_cm2" id="Gran_NaF_98_all" ionChannel="Gran_NaF_98" erev="55.0 mV" ion="na"/>
<!--
                <channelDensity condDensity="55.7227 mS_per_cm2" id="Gran_NaF_98_all" ionChannel="Gran_NaF_98" erev="55.0 mV" ion="na"/>


                <channelDensity condDensity="17.9811 mS_per_cm2" id="Gran_KCa_98_all" ionChannel="Gran_KCa_98" erev="-90.0 mV" ion="k"/>

-->

                <spikeThresh value="0 mV"/>

                <specificCapacitance value="1.0 uF_per_cm2"/>

                <initMembPotential value="-65.0 mV"/>

            </membraneProperties>

            <intracellularProperties>
<!--
                <species id="ca"
                         ion="ca"
                         concentrationModel="Gran_CaPool"
                         initialConcentration="7.55E-5 mM"
                         initialExtConcentration="2 mM"/>-->

                <resistivity value="0.1 kohm_cm"/>

            </intracellularProperties>

        </biophysicalProperties>

    </cell>


    <pulseGenerator id="pulseGen1" delay="100ms" duration="100ms" amplitude="-0.01 nA" />
    <pulseGenerator id="pulseGen2" delay="400ms" duration="100ms" amplitude="0.02 nA" />

    <voltageClamp id="vClamp1" delay="150ms" duration="50ms" targetVoltage="10mV" seriesResistance="1ohm"/>

    <network id="net1">
        <extracellularProperties temperature = "32 degC"/>
        <population id="hhpop" component="Granule_98" size="1"/>
        <explicitInput target="hhpop[0]" input="pulseGen1"/>
        <explicitInput target="hhpop[0]" input="pulseGen2"/>
    </network>


        <!-- End of NeuroML2 content -->


    <Simulation id="sim1" length="700ms" step="0.05ms" target="net1">

        <Display id="d1" title="Ex15: Ca Dynamics: Voltage (mV)" timeScale="1ms" xmin="0" xmax="700" ymin="-90" ymax="60">
            <Line id="l1" quantity="hhpop[0]/v" scale="1mV"  color="#ffffff" timeScale="1ms"/>
        </Display>

        <Display id="d2" title="Ex15: Ca Dynamics: rate variables" timeScale="1ms" xmin="0" xmax="700" ymin="-0.1" ymax="1.1">

            <Line id="l3" quantity="hhpop[0]/biophys/membraneProperties/Gran_KDr_98_all/Gran_KDr_98/m/q" scale="1"  color="#00ff00" timeScale="1ms"/>
            <Line id="l4" quantity="hhpop[0]/biophys/membraneProperties/Gran_KDr_98_all/Gran_KDr_98/h/q" scale="1"  color="#ff00ff" timeScale="1ms"/>
            <Line id="l7" quantity="hhpop[0]/biophys/membraneProperties/Gran_H_98_all/Gran_H_98/n/q" scale="1"  color="#ff0000" timeScale="1ms"/>
        
            <Line id="l5" quantity="hhpop[0]/biophys/membraneProperties/Gran_CaHVA_98_all/Gran_CaHVA_98/m/q" scale="1"  color="#770fff" timeScale="1ms"/>
            <Line id="l6" quantity="hhpop[0]/biophys/membraneProperties/Gran_CaHVA_98_all/Gran_CaHVA_98/h/q" scale="1"  color="#55f00f" timeScale="1ms"/>
            
            <Line id="l8" quantity="hhpop[0]/biophys/membraneProperties/Gran_KA_98_all/Gran_KA_98/m/q" scale="1"  color="#ffff00" timeScale="1ms"/>
            <Line id="l9" quantity="hhpop[0]/biophys/membraneProperties/Gran_KA_98_all/Gran_KA_98/h/q" scale="1"  color="#ffffff" timeScale="1ms"/>

            <!--<Line id="l1" quantity="hhpop[0]/biophys/membraneProperties/Gran_NaF_98_all/Gran_NaF_98/m/q" scale="1"  color="#000000" timeScale="1ms"/>
            <Line id="l2" quantity="hhpop[0]/biophys/membraneProperties/Gran_NaF_98_all/Gran_NaF_98/h/q" scale="1"  color="#0000ff" timeScale="1ms"/>-->

        </Display>
